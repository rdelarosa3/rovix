
    
  <div class="container-login100" style="background-image: url('https://images.unsplash.com/photo-1491772135384-20292e4b5ea1?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=111ee66d6a3cf0d92fb1bb9db9e1486d&auto=format&fit=crop&w=1568&q=80');">

		<div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
        <%= simple_form_for(resource, as: resource_name,class:"login100-form validate-form", url: session_path(resource_name)) do |f| %>
				<span class="login100-form-title p-b-37">
					Log In
				</span>
        


				<div class="wrap-input100 validate-input m-b-20" data-validate="Enter username or email">
           <%= f.input :email,
                        label:false,
                        required: false,
                        autofocus: true,
                        input_html: { autocomplete: "email" , class:"input100",placeholder:"EMAIL"} %>
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100 validate-input m-b-25" data-validate = "Enter password">
          <%= f.input :password,
                    label:false,
                    required: false,
                    input_html: { autocomplete: "current-password",placeholder:"PASSWORD",class:"input100" } %>
					<span class="focus-input100"></span>
				</div>
					
				<div class="container-login100-form-btn">
					 <%= f.button :submit, "Log in" ,class:"login100-form-btn"%>
				</div>
         <% end %>
         <div class="text-center pt-3 ">
        <p>Not a member? <span > <%= link_to "Sign up", new_registration_path(resource_name) %></span></p>
        <p></p>
          <a href="/users/auth/facebook" class="btn btn-just-icon btn-link btn-dribbble"><i class="fab fa-facebook-f"></i></a>
          <% link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
        

	    </div>  

	</div>


		
